{%- if !decls.is_empty() -%}
final class {{name}}Union extends ffi.Union {
    {%- for decl in decls %}
    {{decl}}
    {%- endfor %}
}
{% endif -%}

final class {{name}} extends ffi.Struct {
    {% if !decls.is_empty() -%}
    external {{name}}Union union;
    {%- endif %}

    @ffi.Bool()
    external bool isOk;
}
