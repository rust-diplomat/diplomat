		final result = {##}
		{%- endif -%}
		_{{ m.method_name -}}Ffi(
			{%- for param in m.dart_to_ffi_params %}
			{%- if !loop.first %},{% endif -%}
			{{ param }}
			{%- endfor -%}
		);
		{%- if !m.slice_conversions.is_empty() -%}
		alloc.releaseAll();
		{%- endif %}
		{%- match m.dart_return_expression %}
