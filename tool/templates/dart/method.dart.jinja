			{%- if !loop.first %}, {% endif -%}
			{{ param }}
		{%- endfor -%}
	)>>('{{ m.c_method_name }}')
	.asFunction<{{ m.ffi_cast_return_ty }} Function(
		{%- for param in m.param_types_ffi_cast %}
